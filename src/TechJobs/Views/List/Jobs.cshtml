<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{

        @foreach (Dictionary<string, string> job in ViewBag.jobs)
        {
            <div>
                <table class="job-listing">
                    @foreach (KeyValuePair<string, string> searchTerm in job)
                    {
                        <tr>
                            <td>
                                @searchTerm.Key
                            </td>
                            <td>
                                @searchTerm.Value
                            </td>
                        </tr>
                    }
                </table>
            </div>
        }
    }